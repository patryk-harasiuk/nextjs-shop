{
  "name": "nextjs-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --cache-location './node_modules/@eslint/.eslintcache/' --fix .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "format": "prettier --write \"pages/**/*\" \"components/**/*\" \"hooks/**/*\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache-location './node_modules/@eslint/.eslintcache/' --fix",
      "prettier --write"
    ],
    "*.{json,md,yaml,yml,scss,css}": [
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^3.5.0",
    "@sentry/nextjs": "^6.15.0",
    "@stripe/react-stripe-js": "^1.6.0",
    "@stripe/stripe-js": "^1.21.1",
    "next": "^12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "stripe": "^8.190.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/micro": "^7.3.6",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "16.11.7",
    "@types/react": "17.0.35",
    "@types/stripe": "^8.0.417",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "autoprefixer": "^10.4.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-testing-library": "^5.6.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "postcss": "^8.3.11",
    "prettier": "^2.5.0",
    "prisma": "^4.3.1",
    "tailwindcss": "^3.1.8",
    "typescript": "4.5.2"
  }
}
